<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.bobtest" version="0.1.82" name="Bob's Collection (BOB)" provider-name="BigOldBoy">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.bigoldboy" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
    <extension point="xbmc.service" library="default.py" start="startup"/>
    <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Ron's Addon</summary>
        <description>One Place for Everything</description>
        <platform>all</platform>
    </extension>
</addon>

<addon id="plugin.video.testerbobby" name="My Test Add-on" version="0.0.1" provider-name="Me">
	<requires>
		<import addon="xbmc.python" version="2.19.0" />
		<import addon="script.module.nanscrapers" version="0.5.5" />
        <import addon="script.module.python.koding.aio" version="0.0.1"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>audio executable video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Quick one liner about the add-on</summary>
		<description lang="en">Full add-on description.</description>
		<platform>all</platform>
		<forum>http://www.noobsandnerds.com/support</forum>
		<website>www.noobsandnerds.com</website>
		<news></news>
		<genre></genre>
		<video_guide></video_guide>
		<video_preview></video_preview>
	</extension>
</addon>

